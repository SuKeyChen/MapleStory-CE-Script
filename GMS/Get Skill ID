[ENABLE]
RegisterSymbol(SkillID)
Alloc(GetSkillID, 128)
Alloc(SkillID, 04)

GetSkillID:
mov [SkillID],ecx
cmp byte ptr [ebp+28],00
jmp 02363054 // below

02363050:
jmp GetSkillID
nop

[DISABLE]
02363050: // 80 ? ? ? 89 ? ? ? ? ? 74 ? FF ? ? E8 ? ? ? ? 83 ? ? 85 C0
cmp byte ptr [ebp+28],00


UnRegisterSymbol(SkillID)
DeAlloc(GetSkillID)
DeAlloc(SkillID)