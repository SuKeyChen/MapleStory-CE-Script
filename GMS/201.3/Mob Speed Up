//AUTO BAN WARNING: DO NOT USE WITH HIDE MOST SKILL ANIMATIONS.

[enable]
Alloc(MobSpeedUp, 128)
Label(Return)

MobSpeedUp:
mov dword ptr [ebp+18],00
cmp dword ptr [ebp+18],00
lea edi,[ebx+04]
jmp Return

017D7C53: //83 ? ? 00 8D ? ? 89 83 ? ? 00 00 75 ? 8B
jmp MobSpeedUp
nop
nop
Return:

[disable]
017D7C53:
cmp dword ptr [ebp+18],00
lea edi,[ebx+04]

DeAlloc(MobSpeedUp)
