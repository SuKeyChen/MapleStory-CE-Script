//AUTO BAN WARNING: DO NOT USE WITH HIDE MOST SKILL ANIMATIONS.

[enable]
Alloc(MobSpeedUp, 104)
Label(Return)

MobSpeedUp:
mov dword ptr [ebp+18],00
cmp dword ptr [ebp+18],00
lea edi,[ebx+04]
jmp Return

016845B7: //83 ? ? 00 8D ? ? 89 83 ? ? 00 00 75 ? 8B
jmp MobSpeedUp
nop
nop
Return:

[disable]
016845B7:
cmp dword ptr [ebp+18],00
lea edi,[ebx+04]

DeAlloc(MobSpeedUp)
